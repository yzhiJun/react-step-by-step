{
  "name": "react-step-by-step",
  "version": "1.0.0",
  "description": "React step by step demo",
  "main": "index.js",
  "scripts": {
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "clean": "rimraf dist"
  },
  "keywords": [
    "react",
    "webpack",
    "babel"
  ],
  "author": "Zeejune",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.9.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-externals-plugin": "^1.0.0",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-manifest-plugin": "^1.0.1"
  }
}
